#include <iostream>
#include <numeric>

// Ֆունկցիա՝ երկու թվերի առավելագույն ընդհանուր բաժանարարը (GCD) գտնելու համար
int gcd(int a, int b) {
    // Օգտագործում ենք Էվկլիդեսի ալգորիթմը՝ GCD գտնելու համար
    while (b != 0) {
        int temp = b;  
        b = a % b;  
        a = temp;  
    }
    return a;  
}

int main() {
    int num1, num2;

    
    std::cout << "Մուտքագրեք երկու թիվ: ";
    std::cin >> num1 >> num2;

    
    std::cout << "Առավելագույն ընդհանուր բաժանարարը է: " << gcd(num1, num2) << std::endl;

    return 0;  
}
